class Solution {
    public char findTheDifference(String s, String t) {
        //æ€è·¯1ï¼šå¯¹så’Œtä¸­æ¯ä¸ªå‡ºç°çš„å­—æ¯è¿›è¡Œè®¡æ•°ï¼Œä¸ç›¸åŒçš„å°±æ˜¯æ–°å¢çš„
        //æ€è·¯2ï¼šå°†så’ŒtæŒ‰å­—æ¯é¡ºåºæ’åˆ—ï¼Œç„¶åé€ä¸ªå­—ç¬¦å¯¹æ¯”ï¼Œä¸ç›¸åŒçš„å³æ˜¯æ–°å¢çš„
        //æ€è·¯3ï¼šå°†ä¸¤ä¸ªå­—ç¬¦ä¸²ä¸­çš„å­—ç¬¦Unicodeç å„ç§ç›¸åŠ ï¼Œæœ€åä¸¤è€…çš„å·®å€¼å³æ˜¯æ–°å¢çš„å­—æ¯
        //æ€è·¯4ï¼šå°†ä¸¤ä¸ªå­—ç¬¦ä¸²åˆå¹¶ï¼Œå¯¹å…¶ä¸­æ¯ä¸ªå­—æ¯å‡ºç°æ¬¡æ•°ç›¸åŠ ï¼Œç»“æœä¸ºå¥‡æ•°çš„å°±æ˜¯æ–°å¢çš„å­—æ¯
       int sums = 0;
       int sumt=0;
        for(int i = 0;i<s.length();i++){
            sums = sums + s.charAt(i);
            sumt = sumt + t.charAt(i);
        }
        sumt = sumt + t.charAt(s.length());
        int addsum = sumt - sums;
        return (char)addsum;
    }
}
