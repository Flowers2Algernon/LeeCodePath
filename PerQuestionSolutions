class Solution {
    public char findTheDifference(String s, String t) {
        String mergeSAndT = s+t;
        char c = 0;
        for(int i = 0; i <mergeSAndT.length();i++){
            c =(char)(c ^  mergeSAndT.charAt(i));
        }
        return c;
        }
    }
