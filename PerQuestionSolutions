class Solution {
    public int strStr(String haystack, String needle) {
        int signal = -1;
        int count = 0;
        if(needle.length()>haystack.length()){
            return signal;
        }
        if(needle.length() == 1 && haystack.length() == 1){
            if(needle.equals(haystack)){
                return 0;
            }else{
                return signal;
            }
        }
        for(int j = 0;j<haystack.length()-needle.length();j++){
            if(needle.charAt(0) != haystack.charAt(j)){
                continue;
            }else{
                for(int i = 0;i<needle.length();i++){
                    if(needle.charAt(i)!=haystack.charAt(j+i)){
                        break;
                    }else if(needle.charAt(i)==haystack.charAt(j+i)){//æ­¤æ—¶ä¸¤ä¸ªå­—ç¬¦ä¸²çš„å…¶ä¸­ä¸€ä½å­—ç¬¦èƒ½å¯¹ä¸Š
                        count++;
                    }
                }
                }
                if(count == needle.length()){
                    signal = j;
                }
            }
            return signal;
            }
    }
